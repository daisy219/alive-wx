/**app.wxss**/
@import './style/iconfont.wxss';
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  background: url('https://7369-sicilymarmot-e7t9f-1302241243.tcb.qcloud.la/bg.jpg?sign=2a9ae27cb3e6cc5c8d1fc216446faa06&t=1591615627');
  background-size: cover;
}
page {
  background: #fff;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.clearfix:after { 
  clear: both;
  content: ' ';
  display: block;
}
.fl {
  float: left;
}
.fr {
  float: right;
}
  
.main-navigator {
  padding: 4px 20px;
  border-radius: 4px;
  background-color: #636FA4;
  color: #fff;
}

.no-data {
  width: 100%;
}
.btn-group-grid {
  width: 100%;
}

